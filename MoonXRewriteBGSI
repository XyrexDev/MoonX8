headerFrame.BorderSizePixel = 0

local headerCorner = Instance.new("UICorner", headerFrame)
headerCorner.CornerRadius = UDim.new(0, 15)

-- Header bottom section to square off the bottom
local headerBottom = Instance.new("Frame", headerFrame)
headerBottom.Size = UDim2.new(1, 0, 0, 15)
headerBottom.Position = UDim2.new(0, 0, 1, -15)
headerBottom.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
headerBottom.BorderSizePixel = 0

-- Title label (larger text for bigger GUI)
local title = Instance.new("TextLabel", headerFrame)
title.Size = UDim2.new(1, -40, 0, 50)
title.Position = UDim2.new(0, 20, 0, 15)
title.BackgroundTransparency = 1
