submitCorner.CornerRadius = UDim.new(0, 10)

-- Button hover effects with enhanced animations
local function createHoverEffect(button, hoverColor, originalColor, scaleEffect)
    scaleEffect = scaleEffect or false
    
    button.MouseEnter:Connect(function()
        local hoverTween = TweenService:Create(
            button,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {BackgroundColor3 = hoverColor}
        )
        hoverTween:Play()
        
        if scaleEffect then
            local scaleTween = TweenService:Create(
                button,
                TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                {Size = UDim2.new(button.Size.X.Scale, button.Size.X.Offset + 4, button.Size.Y.Scale, button.Size.Y.Offset + 2)}
            )
            scaleTween:Play()
        end
    end)
    
    button.MouseLeave:Connect(function()
        local leaveTween = TweenService:Create(
            button,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {BackgroundColor3 = originalColor}
        )
        leaveTween:Play()
        
        if scaleEffect then
            local scaleTween = TweenService:Create(
                button,
                TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                {Size = UDim2.new(button.Size.X.Scale, button.Size.X.Offset - 4, button.Size.Y.Scale, button.Size.Y.Offset - 2)}
            )
            scaleTween:Play()
        end
    end)
end

-- Apply hover effects
createHoverEffect(getKeyBtn, Color3.fromRGB(65, 70, 80), Color3.fromRGB(55, 60, 70), true)
createHoverEffect(submitBtn, Color3.fromRGB(80, 170, 80), Color3.fromRGB(60, 150, 60), true)

-- Button functionality
getKeyBtn.MouseButton1Click:Connect(function()
    setclipboard("https://lootdest.org/s?Si1CYuYP")
    
    -- Enhanced visual feedback
    local originalText = getKeyBtn.Text
    local originalColor = getKeyBtn.BackgroundColor3
    
    getKeyBtn.Text = "‚úì Copied to Clipboard!"
    getKeyBtn.BackgroundColor3 = Color3.fromRGB(60, 150, 60)
    
    -- Pulse effect
    local pulseTween = TweenService:Create(
        getKeyBtn,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut),
        {TextSize = 20}
    )
    pulseTween:Play()
    
    wait(0.3)
    
    local pulseTween2 = TweenService:Create(
        getKeyBtn,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut),
        {TextSize = 18}
    )
    pulseTween2:Play()
    
    wait(1.2)
    getKeyBtn.Text = originalText
    getKeyBtn.BackgroundColor3 = originalColor
end)

submitBtn.MouseButton1Click:Connect(function()
    local key = keyBox.Text
    
    -- Start countdown verification
    local originalText = submitBtn.Text
    local originalColor = keyBox.BackgroundColor3
    
    -- Disable button during countdown
    submitBtn.Active = false
    
    -- Countdown from 60 seconds with enhanced visual feedback
    for i = 60, 1, -1 do
        local dots = ""
        if i % 4 == 0 then
            dots = "..."
        elseif i % 4 == 3 then
            dots = ".."
        elseif i % 4 == 2 then
            dots = "."
        else
            dots = "..."
        end
        
        submitBtn.Text = "‚è≥ " .. i .. "s Verifying" .. dots
        submitBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 150)
        wait(1)
    end
    
    -- After countdown, show invalid key message
    submitBtn.Text = "‚ùå Invalid key, Try again"
    submitBtn.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
    
    -- Shake animation for error
    local originalPos = keyBox.Position
    for i = 1, 6 do
        keyBox.Position = UDim2.new(originalPos.X.Scale, originalPos.X.Offset + (i % 2 == 0 and 5 or -5), originalPos.Y.Scale, originalPos.Y.Offset)
        wait(0.1)
    end
    keyBox.Position = originalPos
    
    -- Fade textbox to red
    local redTween = TweenService:Create(
        keyBox,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {BackgroundColor3 = Color3.fromRGB(150, 50, 50)}
    )
    
    redTween:Play()
    
    -- Wait and fade back to original
    wait(1)
    
    local normalTween = TweenService:Create(
        keyBox,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {BackgroundColor3 = originalColor}
    )
    
    normalTween:Play()
    
    -- Reset button text and re-enable
    wait(0.5)
    submitBtn.Text = originalText
    submitBtn.BackgroundColor3 = Color3.fromRGB(60, 150, 60)
    submitBtn.Active = true
end)

-- Start the key system GUI immediately
local function startKeySystemGUI()
    -- Fade-in animation
    local fadeInTween = TweenService:Create(
        frame,
        TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {BackgroundTransparency = 0}
    )

    local shadowFadeIn = TweenService:Create(
        shadow,
        TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {BackgroundTransparency = 0.5}
    )

    -- Animation from top of screen
    frame.Position = UDim2.new(0.5, -275, 0, -380) -- Start above screen
    shadow.Position = UDim2.new(0.5, -270, 0, -375) -- Start above screen

    local slideInTween = TweenService:Create(
        frame,
        TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
        {Position = UDim2.new(0.5, -275, 0.5, -190)}
    )

    local shadowSlideTween = TweenService:Create(
        shadow,
        TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
        {Position = UDim2.new(0.5, -270, 0.5, -185)}
    )

    -- Play animations
    fadeInTween:Play()
    shadowFadeIn:Play()
    slideInTween:Play()
    shadowSlideTween:Play()
end

-- Start immediately
startKeySystemGUI()
